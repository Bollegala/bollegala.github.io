	doi = {10.18653/v1/2023.findings-acl.429},
	month = jul,
	pages = {6868--6882},
	publisher = {Association for Computational Linguistics},
	title = {Unsupervised Semantic Variation Prediction using the Distribution of Sibling Embeddings},
	url = {https://aclanthology.org/2023.findings-acl.429.pdf},
	year = {2023},
	bdsk-file-1 = {},
	bdsk-url-1 = {https://aclanthology.org/2023.findings-acl.429.pdf},
